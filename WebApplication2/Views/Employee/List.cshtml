
@{
    ViewData["Title"] = "List";
}

<h1>Employee List</h1>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Designation</th>
        <th>salary</th>
    </tr>
    @foreach (Employee e in (IEnumerable<Employee>)ViewData["emp list"])
        {
            <tr>
                <td>@e.Id</td>
                    <td>@e.Name</td>
                        <td>@e.Designation</td>
                            <td>@e.salary</td>
            </tr>
        }
        <h2>using View Bag</h2>
        <table class="table table-bordered table-hover">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Designation</th>
                <th>salary</th>
            </tr>
        @foreach (Employee e in ViewBag.emplist)
        {
            <tr>
                <td>@e.Id</td>
                <td>@e.Name</td>
                <td>@e.Designation</td>
                <td>@e.salary</td>
            </tr>
        }
        </table>

</table>
