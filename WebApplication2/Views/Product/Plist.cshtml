
@{
    ViewData["Title"] = "Plist";
}

<h1>Products</h1>

<table class="table table-bordered table-hover"">
    <tr>
        <th>Id</th>
        <th>Model</th>
        <th>Company</th>
        <th>Price</th>
    </tr>
    @foreach (Product p in ViewBag.ProductList)
    {
        <tr>
          <td>@p.Id</td>
        <td>@p.ProductName</td>
        <td>@p.Company</td>
        <td>@p.Price</td>
        </tr>
    }
</table>